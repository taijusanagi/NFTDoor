{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "run-p lint:*",
    "lint:prettier": "prettier '**/*.{json,md}' --ignore-path .gitignore --check",
    "lint:eslint": "eslint '**/*.{js,ts,jsx,tsx}' --ignore-path .gitignore",
    "fix": "run-p fix:*",
    "fix:prettier": "prettier '**/*.{json,md}' --ignore-path .gitignore --write",
    "fix:eslint": "yarn lint:eslint --fix",
    "dev": "run-p dev:*",
    "dev:frontend": "next dev",
    "dev:firebase": "firebase emulators:start --only firestore,storage",
    "build": "next build",
    "start": "next start",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.2.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@mui/material": "^5.9.3",
    "axios": "^0.27.2",
    "ethers": "^5.6.9",
    "firebase": "^9.9.3",
    "firebase-tools": "^11.8.0",
    "framer-motion": "^6",
    "next": "12.2.4",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "recoil": "^0.7.5",
    "three": "^0.143.0",
    "uuid": "^8.3.2",
    "wagmi": "^0.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@chakra-ui/storybook-addon": "^4.0.6",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "18.6.4",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/three": "^0.143.1",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "4.7.4"
  }
